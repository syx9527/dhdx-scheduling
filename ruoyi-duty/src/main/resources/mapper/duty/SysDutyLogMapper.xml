<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.duty.mapper.SysDutyLogMapper">
    <resultMap type="SysDutyLog" id="SysDutyLogResult">
        <result property="logId" column="log_id"/>
        <result property="dutyId" column="duty_id"/>
        <result property="dutyName" column="duty_name"/>
        <result property="userId" column="user_id"/>
        <result property="userName" column="user_name"/>
        <result property="deptId" column="dept_id"/>
        <result property="deptName" column="dept_name"/>
        <result property="startTime" column="start_time"/>
        <result property="endTime" column="end_time"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <sql id="selectSysDutyLogVo">
        select log_id,
               duty_id,
               duty_name,
               user_id,
               user_name,
               dept_id,
               dept_name,
               start_time,
               end_time,
               create_by,
               create_time,
               remark
        from sys_duty_log
    </sql>

    <select id="selectSysDutyLogList" parameterType="SysDutyLog" resultMap="SysDutyLogResult">
        <include refid="selectSysDutyLogVo"/>
        <where>
            <if test="logId != null">
                and log_id = #{logId}
            </if>
            <if test="dutyId != null">
                and duty_id = #{dutyId}
            </if>
            <if test="dutyName != null  and dutyName != ''">
                and duty_name like concat('%', #{dutyName}, '%')
            </if>
            <if test="userId != null">
                and user_id = #{userId}
            </if>
            <if test="userName != null  and userName != ''">
                and user_name like concat('%', #{userName}, '%')
            </if>
            <if test="deptId != null">
                and dept_id = #{deptId}
            </if>
            <if test="createBy != null">
                and create_by = #{createBy}
            </if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''">
                and start_time between #{params.beginCreateTime} and #{params.endCreateTime}
            </if>


            <if test="deptName != null  and deptName != ''">
                and dept_name like concat('%', #{deptName}, '%')
            </if>
            <if test="params.beginStartTime != null and params.beginStartTime != '' and params.endStartTime != null and params.endStartTime != ''">
                and start_time between #{params.beginStartTime} and #{params.endStartTime}
            </if>
            <if test="params.beginEndTime != null and params.beginEndTime != '' and params.endEndTime != null and params.endEndTime != ''">
                and end_time between #{params.beginEndTime} and #{params.endEndTime}
            </if>
        </where>
    </select>

    <select id="selectSysDutyLogByLogId" parameterType="Long" resultMap="SysDutyLogResult">
        <include refid="selectSysDutyLogVo"/>
        where log_id = #{logId}
    </select>

    <insert id="insertSysDutyLog" parameterType="SysDutyLog" useGeneratedKeys="true" keyProperty="logId">
        insert into sys_duty_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="dutyId != null">
                duty_id,
            </if>
            <if test="dutyName != null">
                duty_name,
            </if>
            <if test="userId != null">
                user_id,
            </if>
            <if test="userName != null">
                user_name,
            </if>
            <if test="deptId != null">
                dept_id,
            </if>
            <if test="deptName != null">
                dept_name,
            </if>
            <if test="startTime != null">
                start_time,
            </if>
            <if test="endTime != null">
                end_time,
            </if>
            <if test="remark != null and remark != ''">
                remark,
            </if>
            <if test="createBy != null and createBy != ''">
                create_by,
            </if>
            create_time
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="dutyId != null">
                #{dutyId},
            </if>
            <if test="dutyName != null">
                #{dutyName},
            </if>
            <if test="userId != null">
                #{userId},
            </if>
            <if test="userName != null">
                #{userName},
            </if>
            <if test="deptId != null">
                #{deptId},
            </if>
            <if test="deptName != null">
                #{deptName},
            </if>
            <if test="startTime != null">
                #{startTime},
            </if>
            <if test="endTime != null">
                #{endTime},
            </if>
            <if test="remark != null and remark != ''">
                #{remark},
            </if>
            <if test="createBy != null and createBy != ''">
                #{createBy},
            </if>
            sysdate()
        </trim>
    </insert>

    <update id="updateSysDutyLog" parameterType="SysDutyLog">
        update sys_duty_log
        <trim prefix="SET" suffixOverrides=",">
            <if test="dutyId != null">
                duty_id = #{dutyId},
            </if>
            <if test="dutyName != null">
                duty_name = #{dutyName},
            </if>
            <if test="userId != null">
                user_id = #{userId},
            </if>
            <if test="userName != null">
                user_name = #{userName},
            </if>
            <if test="deptId != null">
                dept_id = #{deptId},
            </if>
            <if test="deptName != null">
                dept_name = #{deptName},
            </if>
            <if test="startTime != null">
                start_time = #{startTime},
            </if>
            <if test="endTime != null">
                end_time = #{endTime},
            </if>
            <if test="remark != null">
                remark = #{remark},
            </if>
            <if test="updateBy != null and updateBy != ''">
                update_by = #{updateBy},
            </if>
            update_time = sysdate()
        </trim>
        where log_id = #{logId}
    </update>

    <delete id="deleteSysDutyLogByLogId" parameterType="Long">
        delete
        from sys_duty_log
        where log_id = #{logId}
    </delete>

    <delete id="deleteSysDutyLogByLogIds" parameterType="String">
        delete
        from sys_duty_log where log_id in
        <foreach item="logId" collection="array" open="(" separator="," close=")">
            #{logId}
        </foreach>
    </delete>
</mapper>
